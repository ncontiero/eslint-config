{
  "name": "@ncontiero/eslint-config",
  "type": "module",
  "version": "7.0.0",
  "packageManager": "pnpm@10.28.0",
  "description": "Nicolas's ESLint config.",
  "author": {
    "name": "Nicolas Contiero",
    "url": "https://github.com/ncontiero"
  },
  "license": "MIT",
  "homepage": "https://github.com/ncontiero/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ncontiero/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/ncontiero/eslint-config/issues"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm typegen && tsup",
    "dev": "pnpm dlx @eslint/config-inspector --config eslint-inspector.config.ts",
    "build:inspector": "pnpm build && pnpm dlx @eslint/config-inspector build --config eslint-inspector.config.ts",
    "typegen": "tsx scripts/typegen.ts",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "typecheck": "tsc --noEmit",
    "check": "pnpm lint && pnpm typecheck",
    "release": "changeset publish",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@tanstack/eslint-plugin-query": ">=5.50.0",
    "eslint": ">=9.20.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/eslint-plugin-query": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/markdown": "^7.5.1",
    "@html-eslint/eslint-plugin": "^0.53.0",
    "@html-eslint/parser": "^0.53.0",
    "@next/eslint-plugin-next": "^16.1.1",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-merge-processors": "^2.0.0",
    "eslint-plugin-antfu": "^3.1.3",
    "eslint-plugin-better-tailwindcss": "4.0.0-rc.0",
    "eslint-plugin-command": "^3.4.0",
    "eslint-plugin-de-morgan": "^2.0.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^62.0.0",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-perfectionist": "^5.3.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-toml": "^0.13.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-yml": "^1.19.1",
    "globals": "^17.0.0",
    "jsonc-eslint-parser": "^2.4.2",
    "local-pkg": "^1.1.2",
    "prettier": "^3.7.4",
    "toml-eslint-parser": "^1.0.0",
    "yaml-eslint-parser": "^1.3.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@ncontiero/changelog-github": "^2.1.2",
    "@ncontiero/prettier-config": "^1.0.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^25.0.6",
    "eslint": "^9.39.2",
    "eslint-typegen": "^2.3.0",
    "execa": "^9.6.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tinyglobby": "^0.2.15",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "lint-staged": {
    "*": "pnpm lint:fix"
  },
  "prettier": "@ncontiero/prettier-config"
}
